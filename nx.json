{
  "npmScope": "whoa",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "hop-ui": {
      "tags": []
    },
    "hop-ui-e2e": {
      "tags": [],
      "implicitDependencies": ["hop-ui"]
    },
    "web-auth-data-access": {
      "tags": ["type:data-access"]
    },
    "web-auth-feature-login": {
      "tags": ["type:feature"]
    },
    "web-auth-feature-shell": {
      "tags": ["type:feature"]
    },
    "web-auth-ui-login-form": {
      "tags": ["type:ui"]
    },
    "web-auth-utils": {
      "tags": ["type:utils"]
    },
    "web-core": {
      "tags": ["type:ui"]
    },
    "web-hop-feature-hop-home": {
      "tags": ["type:feature"]
    },
    "web-hop-feature-hop-shell": {
      "tags": ["scope:hop-ui"]
    },
    "web-service-req-data-access": {
      "tags": ["type:data-access"]
    },
    "web-service-req-data-access-model": {
      "tags": ["type:data-access"]
    },
    "web-service-req-feature-service-request": {
      "tags": ["type:feature"]
    },
    "web-service-req-feature-shell": {
      "tags": ["type:feature"]
    },
    "web-service-req-ui-service-request-form": {
      "tags": ["type:ui"]
    },
    "web-service-req-utils": {
      "tags": ["type:utils"]
    },
    "web-settings-data-access": {
      "tags": ["type:data-access"]
    },
    "web-settings-feature": {
      "tags": ["type:feature"]
    },
    "web-shared-ui-icons-provider": {
      "tags": ["scope:shared-ui"]
    },
    "web-shared-ui-layout": {
      "tags": ["type:ui"]
    },
    "web-shared-ui-zorro-provider": {
      "tags": ["type:ui"]
    }
  }
}
